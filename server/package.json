    "cookie-session": "^2.0.0-beta.3",
    "create-react-app": "^2.1.5",
    "express": "^4.16.4",
    "mongoose": "^5.4.13",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
